{
  "name": "space4u",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Anonymous peer support platform for mental health",
  "keywords": [
    "mental-health",
    "support",
    "community",
    "wellness",
    "react"
  ],
  "author": "Space4U Team",
  "license": "MIT",
  "homepage": "https://space4u.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/space4u.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "npx vite-bundle-analyzer",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "mobile:init": "npx cap init",
    "mobile:add:android": "npx cap add android",
    "mobile:add:ios": "npx cap add ios",
    "mobile:sync": "npm run build && npx cap sync",
    "mobile:run:android": "npx cap run android",
    "mobile:run:ios": "npx cap run ios",
    "mobile:open:android": "npx cap open android",
    "mobile:open:ios": "npx cap open ios",
    "android:debug": "npm run build && npx cap sync android && cd android && .\\gradlew assembleDebug",
    "android:install": "cd android && .\\gradlew installDebug",
    "android:release": "npm run build && npx cap sync android && cd android && .\\gradlew bundleRelease",
    "android:apk": "npm run build && npx cap sync android && cd android && .\\gradlew assembleRelease",
    "i18n:test": "vitest run src/tests/i18n.test.js",
    "i18n:report": "vitest run src/tests/i18n.test.js --reporter=verbose",
    "i18n:validate": "node scripts/validateTranslations.js",
    "i18n:missing": "node scripts/findMissingTranslations.js"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/local-notifications": "^6.0.0",
    "@capacitor/network": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@capacitor/push-notifications": "^6.0.0",
    "@capacitor/share": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@supabase/supabase-js": "^2.74.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.0.0",
    "react-router-dom": "^6.8.1",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.14",
    "happy-dom": "^20.0.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
